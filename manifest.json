{
    "manifest_version": 3,
    "name": "Claude Session Key Manager",
    "version": "1.0",
    "description": "管理和切换Claude会话密钥",
    "permissions": [
        "storage",
        "activeTab",
        "identity",
        "identity.email"
    ],
    "oauth2": {
        "client_id": "YOUR_CLIENT_ID",
        "scopes": [
            "https://www.googleapis.com/auth/drive.file"
        ]
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://ccc.008778.xyz/*",
                "https://ccc.008778.xyz/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}